	script.
		$(document).on("ready", function(){

			// hide navbar on scroll
			var last_position = 0;
			$(window).on("scroll", function(){
				var scrollPosition = $(window).scrollTop();
				
				if(scrollPosition >= 50){
					$(".cm-navbar").css("top", "-50px")
				}
				if(last_position > scrollPosition){
					$(".cm-navbar").css("top", "0px")
				}

				last_position = scrollPosition;
			})

			// post - feeling selector 
			$(".cm-panel.-timeline-post ._feeling-option").on("click", function(){
				$(this).addClass("-active").siblings("._feeling-option").removeClass("-active");
			})

			// open post
			$(".cm-timeline-post").on("click", function(){
				$(".cm").addClass("-locked")
				$(".cm-panel.-timeline-post").addClass("-open")
			})
			// close post
			$(".-timeline-post .cm-topbar-item").on("click", function(){
				$(".cm").removeClass("-locked")
				$(".cm-panel.-timeline-post").removeClass("-open")
			})

			// open post-caption
			$(".cm-panel.-timeline-post ._caption-button, .cm-panel.-timeline-post ._caption-edit").on("click", function(){
				$(".-timeline-post-caption").addClass("-open");
			})
			// close post-caption
			$(".cm-panel.-timeline-post-caption .cm-topbar-item").on("click", function(){
				$(".-timeline-post-caption").removeClass("-open")
			})

			// open post-item
			$(".cm-panel.-timeline-post ._button.-item").on("click", function(){
				$(".cm-panel.-timeline-post-item").addClass("-open");
				//$(".-timeline-post-item ._search-input input").focus();
			})
			// close post-item
			$(".cm-panel.-timeline-post-item .cm-topbar-item").on("click", function(){
				$(".cm-panel.-timeline-post-item").removeClass("-open")
			})
			// open post-item-clear
			$(".cm-panel.-timeline-post-item ._sticky ._clear").on("click", function(){
				$(".cm-modal.-timeline-post-item-clear").addClass("-open");
			})
			// close post-item-clear
			$(".cm-modal.-timeline-post-item-clear [toggle-close]").on("click", function(){
				$(".cm-modal.-timeline-post-item-clear").removeClass("-open");
			})
			// repaint layout post-item
			var $drawerPostItem = $(".-timeline-post-item");
			$(window).on("resize.post_item", function(){
				$drawerPostItem.find(".cm-wrapper").css("margin-bottom", $drawerPostItem.find("._sticky").outerHeight() -1)
			}).trigger("resize.post_item")

			// tab -timeline-post-item
			$("[toggle-post-item]").on("click", function(){
				var target = $(this).attr("toggle-post-item")
				$(target).siblings("._pane").removeClass("-open")
				$(target).addClass("-open");

				var toggler = $("[toggle-post-item='" + target + "']");
				toggler.siblings("[toggle-post-item]").removeClass("-active")
				toggler.addClass("-active");

				$drawerPostItem.find(".cm-wrapper").css("min-height", $(target).outerHeight())

				if($(target).hasClass("-filled")){
					$drawerPostItem.find(".cm-sticky").css("background", "#ECEFF1")
				} else{
					$drawerPostItem.find(".cm-sticky").css("background", "#FFFFFF")
				}
			})

			// open post-place
			$(".-timeline-post ._button.-with").on("click", function(){
				$(".-timeline-post-with").addClass("-open");
			})
			// close post-place
			$(".-timeline-post-with .cm-topbar-item").on("click", function(){
				$(".-timeline-post-with").removeClass("-open")
			})
			
			// open post-place
			$(".-timeline-post ._button.-place").on("click", function(){
				$(".-timeline-post-place").addClass("-open");
			})
			// close post-place
			$(".-timeline-post-place .cm-topbar-item").on("click", function(){
				$(".-timeline-post-place").removeClass("-open")
			})

			// card tab - initial height (comment tab's)
			$(window).on("resize.card-tab", function(){
				$(".cm-card").each(function(){
					$(this).children("._tab").css("height", $(this).find("._tab-item").outerHeight() + $(this).children("._tab").find(".-active ._tab-pane").outerHeight())
				})
			}).trigger("resize.card-tab")
			// card tab - toggle tab
			$(".cm-card ._tab-button").on("click", function(){
				$(this).parent().addClass("-active").siblings("._tab-item").removeClass("-active")
				$(this).closest("._tab").css("height", $(this).parent().outerHeight() + $(this).siblings("._tab-pane").outerHeight())
			})
			// card reaction - toggler
			$(".cm-card ._react ._dropdown-button").on("click", function(){
				$(this).parent("._dropdown").toggleClass("-open")
				$(this).siblings("._dropdown-reaction").find("._reaction").removeClass("-active")
			})
			// card reaction - selector
			$("._reaction").on("click", function (){
					$(this).parent().siblings("._col").removeClass('-active');
					$(this).parent().addClass('-active');
					$(this).on('animationiteration webkitAnimationIteration', function(){
						$(this).parent().removeClass("-active");
						$(this).closest("._dropdown").removeClass("-open")
					});
			});
			// open cimotion
			$(".cm-card ._react ._paragraph").on("click", function(){
				$(".cm").addClass("-locked")
				$(".cm-panel.-card-cimotion").addClass("-open")
			})
			// close cimotion
			$(".cm-panel.-card-cimotion .cm-topbar-item").on("click", function(){
				$(".cm").removeClass("-locked")
				$(".cm-panel.-card-cimotion").removeClass("-open")
			})
			// open detail-comment
			$(".cm-card ._tab-item.-comment ._read").on("click", function(){
				$(".cm").addClass("-locked")
				$(".cm-panel.-card-detail").addClass("-open")
			})
			// open detail-share
			$(".cm-card ._tab-item.-share ._shared").on("click", function(){
				$(".cm").addClass("-locked")
				$(".cm-panel.-card-detail").addClass("-open")
			})
			// close detail
			$(".cm-panel.-card-detail .cm-topbar-item").on("click", function(){
				$(".cm").removeClass("-locked")
				$(".cm-panel.-card-detail").removeClass("-open")
			})
			// open sticker
			$(".ci-sticker-button").on("click", function(){
				$(this).closest(".cm-card").find(".cm-sticker").toggleClass("cm-hide")
				$(this).closest(".cm-panel-content").find(".cm-sticker").toggleClass("cm-hide")
			})
			// card - toggle layer
			$("[layer-target]").on("click", function(){
				var target = $(this).attr("layer-target");
				$(this).closest(".cm-card").find("._media ._info-layer").children("."+target).addClass("-active").siblings("._layer").removeClass("-active")
			})
			$(".cm-card ._media ._info-layer ._close").on("click", function(){
				$(this).siblings("._layer").removeClass("-active")
			})
			// open modal ask-with
			$(".cm-card ._cell.-with.-ask").on("click", function(){
				$(".cm-modal.-card-ask-with").addClass("-open");
			})
			// close post-item-clear
			$(".cm-modal.-card-ask-with [toggle-close]").on("click", function(){
				$(".cm-modal.-card-ask-with").removeClass("-open");
			})
			// open modal ask-with
			$(".cm-card ._cell.-place.-ask").on("click", function(){
				$(".cm-modal.-card-ask-place").addClass("-open");
			})
			// close post-item-clear
			$(".cm-modal.-card-ask-place [toggle-close]").on("click", function(){
				$(".cm-modal.-card-ask-place").removeClass("-open");
			})
			// open card-ask-with
			$("[open-ask-with]").on("click", function(){
				$(".cm").addClass("-locked")
				$(".cm-panel.-card-ask.-with").addClass("-open")
			})
			// close card-ask-with
			$(".cm-panel.-card-ask.-with .cm-topbar-item").on("click", function(){
				$(".cm").removeClass("-locked")
				$(".cm-panel.-card-ask.-with").removeClass("-open");
			})
			// open card-ask-place
			$("[open-ask-place]").on("click", function(){
				$(".cm").addClass("-locked")
				$(".cm-panel.-card-ask.-place").addClass("-open")
			})
			// close card-ask-place
			$(".cm-panel.-card-ask.-place .cm-topbar-item").on("click", function(){
				$(".cm").removeClass("-locked")
				$(".cm-panel.-card-ask.-place").removeClass("-open");
			})

			$("[open-place]").click(function(){
				$(".cm-panel.-timeline-post-place").addClass("-open")
			})

			$("[open-with]").click(function(){
				$(".cm-panel.-timeline-post-with").addClass("-open")
			})

			$("._comment-action-button.-reply").on("click", function(){
				$(".cm").addClass("-locked")
				$(".cm-panel.-card-comment-reply").addClass("-open")
			})

			$(".cm-panel.-card-comment-reply .cm-topbar-item").on("click", function(){
				$(".cm").removeClass("-locked")
				$(".cm-panel.-card-comment-reply").removeClass("-open")
			})
			$(".cm-panel.-card-comment-edit .cm-topbar-item").on("click", function(){
				$(".cm").removeClass("-locked")
				$(".cm-panel.-card-comment-edit").removeClass("-open")
			})

			$(".cm-card-comment.-reply ._comment ._input, .cm-card-comment.-edit ._comment ._input").on({
				"focus": function(){
					$(this).parent().addClass("-focus")
				},
				"blur": function(){
					$(this).parent().removeClass("-focus")
				}
			})
		})

		function editComment(){
			$(".cm-panel.-card-comment-edit").addClass("-open")
		}