include _mixins
+layout({
	navbar: "timeline"
})
	.cm-timeline
		.cm-timeline-post
			._button
				+icon("add-item", 20)
				._label What are you doing?
		mixin card(Ver, With, Place, Comment)
			.cm-card
				._head
					+pp(30)
					._username aji_mahendra
					._userstatus is leaving on a jetplane
				if Ver !== "detail"
					._media
						._timestamp
							._time 08:15
							._date 01 JAN 2015
						img(src="assets/img/timeline/_activity.png" style="display:block;width:100%")
						._info-layer
							mixin layer-item(No)
								._layer.-item(class="-item-" + No)
									+img("timeline/_item-1-large.png", {class: "_image"})
									._title Asics Run Shoes
									._paragraph Lorem ipsum dolor sit amet, consectetur adipisicing elit. Unde magnam sapiente totam reiciendis voluptates quis numquam at, mollitia veritatis nulla quam saepe harum, culpa, neque doloribus quod. Saepe, deserunt, hic?
							+layer-item(1)
							+layer-item(2)
							+layer-item(3)
							if With == "filled"
								._layer.-with
									._title With 5 people
									._list
										+pp(30)
										._username Daniel Tatang's
									._list
										+pp(30)
										._username Daniel Tatang's
									._list
										+pp(30)
										._username Daniel Tatang's
									._list
										+pp(30)
										._username Daniel Tatang's
									._list
										+pp(30)
										._username Daniel Tatang's
									._list
										+pp(30)
										._username Daniel Tatang's
									._list
										+pp(30)
										._username Daniel Tatang's
							if With == "secret"
								._layer.-with-secret
									+img("card/its-secret.png", {class: "_image"})
									._paragraph It's secret
							if With == "alone"
								._layer.-with-alone
									+img("card/im-alone.png", {class: "_image"})
									._paragraph I'm alone
							if Place == "filled"
								._layer.-place
									+img("timeline/_place-1-layer.png", {class:"_logo"})
									._title BMW Motor SARL
									._address 23-29 Rives de Clausen, L-2165 Luxembourg | Company No: R.C.S. Luxembourg  | B100.468, VAT: LU 20981643
									._map: img(src="assets/img/timeline/_map.png" style="width:100%;display:block")

							if Place == "secret"
								._layer.-place.-secret
									+img("card/secret-place.png", {class:"_logo"})
									._title It's Secret
									._address [Keterangan tentang bahwa user merahasiakan tempat ini untuk diketahui oleh orang lain]
									._map This location is not available on map
									
							._close: +icon("delete-2", 10)
							._overlay
					._info
						+img("timeline/column.png", {class: "_holder"})
						._row
							._column
								._cell.-item: ._block(style="background-image: url(assets/img/timeline/_item-1.png)" layer-target="-item-1")
								._cell.-item: ._block(style="background-image: url(assets/img/timeline/_item-2.png)" layer-target="-item-2")
								._cell.-item: ._block(style="background-image: url(assets/img/timeline/_item-4.png)" layer-target="-item-3")
								._cell.-item.-empty: ._block(style="background-image: url(assets/img/timeline/item-empty.png)")

								if With == "filled"
									._cell.-with.-friend
										._block(layer-target="-with")
											span
												| with
												b 5
											+icon("with", 20)

								if With == "ask"
									._cell.-with.-ask
										._block
											+icon("icon-with-side", 42)
											span
												| who 
												br
												| are you 
												br
												| with?

								if With == "secret"
									._cell.-with.-secret
										._block(layer-target="-with-secret")
											+icon("icon-with-side", 42)
											+icon("icon-balon", 44)
											span 
												| It's 
												br
												| Secret
												
								if With == "alone"
									._cell.-with.-alone
										._block(layer-target="-with-alone")
											+icon("icon-with-side", 42)
											+icon("icon-balon", 44)
											span
												| I'm 
												br
												| Alone
								
								if Place == "filled"
									._cell.-place.-filled
										._block(layer-target="-place" style="background-image: url(assets/img/timeline/_place-1.png)")
											+icon("icon-place-side", 42)

								if Place == "ask"
									._cell.-place.-ask
										._block
											+icon("icon-place-side", 42)
											span 
												| where 
												br
												| are you?

								if Place == "secret"
									._cell.-place.-secret
										._block(layer-target="-place.-secret")
											+icon("icon-place-side", 42)
											+icon("icon-balon", 44)
											span 
												| It's
												br
												| Secret
					._react
						._overview: +img("timeline/_react.png")
						._dropdown
							._dropdown-button: +icon("cimotion", 20)
							._dropdown-reaction
								._arrow
								._col: ._reaction.-s40(style="background-image: url('assets/img/card/Ciayo.png');")
								._col: ._reaction.-s25(style="background-image: url('assets/img/card/Chips.png');")
								._col: ._reaction.-s50(style="background-image: url('assets/img/card/Crocket.png');")
								._col: ._reaction.-s30(style="background-image: url('assets/img/card/Coco.png');")
								._col: ._reaction.-s45(style="background-image: url('assets/img/card/Carla.png');")
								._col: ._reaction.-s55(style="background-image: url('assets/img/card/Chuck.png');")
