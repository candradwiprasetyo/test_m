						._paragraph Be the first to CIMOTE this!
				._tab
					._tab-item.-comment.-active
						._tab-button
							span
								+icon("comment", 20)
								b 54
						._tab-pane
							if Comment
								if Ver !== "detail"
									._read
										+icon("comment", 20)
										span Read all 36 comments ..
								mixin comment(Class, Open)
									.cm-card-comment(class= Class)
										+pp(30)
										._comment
											._comment-username aji_mahendra
											._comment-pubdate 5 Dec at 08:00
											._comment-option(class= Open)
												+icon("etc-fill", 12)
												._comment-option-dropdown
													._option(onclick="editComment()") Edit
													._option Delete
											._comment-content Vivamus sagittis lacus vel augue laoreet rutrum faucibus dolor auctor. Aen lacinia bibendum nulla sed
											._comment-action
												._comment-action-button.-reply
													+icon("reply")
													span Reply
												._comment-action-button.-like.cm-color-dark-blue
													+icon("like")
													span 20
												._comment-action-button.-dislike
													+icon("dislike")
													span 16
												.cm-clearfix
											if Post
												._comment-replies View 2 replies ..
								+comment("-post", "-open")
								+comment
							else
								._empty
									+img("timeline/empty-comment.png", {class: "_empty-image"})
									._empty-paragraph Be the first to comment
							mixin comment-reply
								.cm-card-comment.-reply.-down
									+pp(30)
									button(type="button").cm-button.-primary.-small Send
									._comment
										//- ._input(contenteditable="true" placeholder="Write a comment ..")
										input(type="text" class="_input" placeholder="Write a comment ..")
										+icon("sticker-button", 16)		
							if Ver == "detail"
								._sticky
									+comment-reply
									+sticker("cm-hide")
							else
								+comment-reply
							
					._tab-item.-share
						._tab-button
							span
								+icon("share", 20)
								b 20
						._tab-pane
							._title Share this post
							._social
								._social-button: +icon("facebook", 40)
								._social-button: +icon("twitter", 40)
								._social-button: +icon("instagram", 40)
								._social-button.-more 
									+icon("meatball", 40)
							._copy
								span Copy this postâ€™s link
								+icon("setting-privacy-policy", 16)
							if Ver == "timeline"
								._shared
									+icon("share", 16)
									span View all 20 shares ..
							if Ver == "detail"
								mixin shared(Open)
									._list
										+pp(30)
										._right
											a(href)._link: +icon("facebook", 18) 
											a(href)._link: +icon("instagram", 18) 
											._dropdown(class= Open)
												+icon("meatball", 18)
												._dropdown-social
													a(href)
														+icon("pinterest", 18) 
														span Pinterest
													a(href)
														+icon("twitter", 18) 
														span Twitter
										._username ananda mikola
								+shared("-open")
								+shared()
								+shared()
								+shared()
								+shared()
								+shared()

					._tab-item.-download
						._tab-button: span: +icon("download", 20)
						._tab-pane
							+img("timeline/_post.png", {class: "_image"})
							button(type="button").cm-button.-primary.-small
								+icon("download")
								| Download Activity
					._tab-item.-option
						._tab-button: span: +icon("etc-outline", 20)
						._tab-pane
							._show-to
								._show-to-label Show to:
								._show-to-button
									+icon("public", 16)
									| Public
									select
										option Public
										option Friends
										option Only Me
									+icon("dropdown", 10)
